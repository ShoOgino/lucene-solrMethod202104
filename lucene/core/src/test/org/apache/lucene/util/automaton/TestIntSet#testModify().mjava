  @Test
  public void testModify() {
    SortedIntSet set = new SortedIntSet(2);
    set.incr(1);
    set.incr(2);
    set.computeHash();

    FrozenIntSet set2 = set.freeze(0);
    assertEquals(set, set2);

    set.incr(1);
    set.computeHash();
    assertEquals(set, set2);

    set.decr(1);
    set.computeHash();
    assertEquals(set, set2);

    set.decr(1);
    set.computeHash();
    assertNotEquals(set, set2);
  }

