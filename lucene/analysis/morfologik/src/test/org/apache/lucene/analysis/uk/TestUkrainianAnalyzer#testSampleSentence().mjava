  public void testSampleSentence() throws Exception {
    Analyzer a = new UkrainianMorfologikAnalyzer();
    assertAnalyzesTo(
        a,
        "Це — проект генерування словника з тегами частин мови для української мови.",
        new String[] {
          "проект",
          "генерування",
          "словник",
          "тег",
          "частина",
          "мова",
          "українська",
          "український",
          "Українська",
          "мова"
        });
    a.close();
  }

