  public FunctionRangeQuery(
      ValueSource valueSource,
      Number lowerVal,
      Number upperVal,
      boolean includeLower,
      boolean includeUpper) {
    this(
        valueSource,
        lowerVal == null ? null : lowerVal.toString(),
        upperVal == null ? null : upperVal.toString(),
        includeLower,
        includeUpper);
  }

