  public static void assertAnalyzesTo(
      Analyzer a,
      String input,
      String[] output,
      int startOffsets[],
      int endOffsets[],
      String types[],
      int posIncrements[])
      throws IOException {
    assertTokenStreamContents(
        a.tokenStream("dummy", input),
        output,
        startOffsets,
        endOffsets,
        types,
        posIncrements,
        null,
        input.length());
    checkResetException(a, input);
    checkAnalysisConsistency(random(), a, true, input);
  }

