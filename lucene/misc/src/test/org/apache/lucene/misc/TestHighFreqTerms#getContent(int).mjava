  /**
   * getContent return string containing numbers 1 to i with each number n occurring n times. i.e.
   * for input of 3 return string "3 3 3 2 2 1"
   */
  private static String getContent(int i) {
    String s = "";
    for (int j = 10; j >= i; j--) {
      for (int k = 0; k < j; k++) {
        // if j is 3 we return "3 3 3"
        s += String.valueOf(j) + " ";
      }
    }
    return s;
  }

