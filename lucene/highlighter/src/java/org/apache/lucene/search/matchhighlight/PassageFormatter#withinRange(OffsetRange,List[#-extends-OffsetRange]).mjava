  private boolean withinRange(OffsetRange limits, List<? extends OffsetRange> contained) {
    contained.forEach(
        r -> {
          if (r.from < limits.from || r.to > limits.to) {
            throw new AssertionError(
                String.format(
                    Locale.ROOT,
                    "Range outside of the permitted limit (limit = %s): %s",
                    limits,
                    r));
          }
        });
    return true;
  }

