  /** null x not allowed */
  public void testPolygonNullPolyLats() {
    IllegalArgumentException expected =
        expectThrows(
            IllegalArgumentException.class,
            () -> {
              new XYPolygon(null, new float[] {-66, -65, -65, -66, -66});
            });
    assertTrue(expected.getMessage().contains("x must not be null"));
  }

