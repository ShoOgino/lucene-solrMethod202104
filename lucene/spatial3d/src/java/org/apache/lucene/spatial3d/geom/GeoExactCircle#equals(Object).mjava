  @Override
  public boolean equals(Object o) {
    if (!(o instanceof GeoExactCircle)) {
      return false;
    }
    GeoExactCircle other = (GeoExactCircle) o;
    return super.equals(other)
        && other.center.equals(center)
        && other.radius == radius
        && other.actualAccuracy == actualAccuracy;
  }

