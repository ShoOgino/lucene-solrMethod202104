  public static LongHeap create(Order order, int maxSize) {
    // TODO: override push() for unbounded queue
    if (order == Order.MIN) {
      return new LongHeap(maxSize) {
        @Override
        public boolean lessThan(long a, long b) {
          return a < b;
        }
      };
    } else {
      return new LongHeap(maxSize) {
        @Override
        public boolean lessThan(long a, long b) {
          return a > b;
        }
      };
    }
  }

