  public void testRepeatedInfixesAreNotEmitted() throws IOException {
    assertAnalyzesTo(
        analyzer,
        "alarm alas harm",
        new String[] {
          "alarm", "alarmXX", "alarXX", "alaXX", "alXX", "aXX", "larmXX", "larXX", "laXX", "lXX",
          "armXX", "arXX", "rmXX", "rXX", "mXX", "XX", "alas", "alasXX", "lasXX", "asXX", "sXX",
          "harm", "harmXX", "harXX", "haXX", "hXX"
        });
  }

