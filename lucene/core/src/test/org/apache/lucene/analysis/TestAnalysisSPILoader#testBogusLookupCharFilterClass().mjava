  public void testBogusLookupCharFilterClass() {
    expectThrows(
        IllegalArgumentException.class,
        () -> {
          CharFilterFactory.lookupClass("sdfsdfsdfdsfsdfsdf");
        });

    expectThrows(
        IllegalArgumentException.class,
        () -> {
          CharFilterFactory.lookupClass("!(**#$U*#$*");
        });
  }

