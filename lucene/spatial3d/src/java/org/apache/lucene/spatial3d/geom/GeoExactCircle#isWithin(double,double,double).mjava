  @Override
  public boolean isWithin(final double x, final double y, final double z) {
    for (final CircleSlice slice : circleSlices) {
      if (slice.circlePlane.isWithin(x, y, z)
          && slice.plane1.isWithin(x, y, z)
          && slice.plane2.isWithin(x, y, z)) {
        return true;
      }
    }
    return false;
  }

