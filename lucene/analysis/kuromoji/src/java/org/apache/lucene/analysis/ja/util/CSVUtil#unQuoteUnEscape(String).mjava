  private static String unQuoteUnEscape(String original) {
    String result = original;

    // Unquote
    if (result.indexOf('\"') >= 0) {
      Matcher m = QUOTE_REPLACE_PATTERN.matcher(original);
      if (m.matches()) {
        result = m.group(1);
      }

      // Unescape
      if (result.indexOf(ESCAPED_QUOTE) >= 0) {
        result = result.replace(ESCAPED_QUOTE, "\"");
      }
    }

    return result;
  }

