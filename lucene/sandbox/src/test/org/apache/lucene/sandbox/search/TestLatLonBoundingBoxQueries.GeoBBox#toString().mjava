    @Override
    public String toString() {
      StringBuilder b = new StringBuilder();
      b.append("GeoBoundingBox(lat: ");
      b.append(minLat);
      b.append(" TO ");
      b.append(maxLat);
      b.append(", lon: ");
      b.append(minLon);
      b.append(" TO ");
      b.append(maxLon);
      b.append(")");

      return b.toString();
    }

