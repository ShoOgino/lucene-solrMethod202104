    /** pretty print the triangle vertices */
    public String toString() {
      String result =
          (aX + ", " + aY)
              + " "
              + (bX + ", " + bY)
              + " "
              + (cX + ", " + cY)
              + " "
              + ("[" + ab + "," + bc + "," + ca + "]");
      return result;
    }

