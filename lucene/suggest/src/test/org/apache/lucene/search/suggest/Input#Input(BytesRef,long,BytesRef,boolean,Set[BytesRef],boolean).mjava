  public Input(
      BytesRef term,
      long v,
      BytesRef payload,
      boolean hasPayloads,
      Set<BytesRef> contexts,
      boolean hasContexts) {
    this.term = term;
    this.v = v;
    this.payload = payload;
    this.hasPayloads = hasPayloads;
    this.contexts = contexts;
    this.hasContexts = hasContexts;
  }

