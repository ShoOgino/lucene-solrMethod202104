  public void testBogusLookupTokenFilterClass() {
    expectThrows(
        IllegalArgumentException.class,
        () -> {
          TokenFilterFactory.lookupClass("sdfsdfsdfdsfsdfsdf");
        });

    expectThrows(
        IllegalArgumentException.class,
        () -> {
          TokenFilterFactory.lookupClass("!(**#$U*#$*");
        });
  }

