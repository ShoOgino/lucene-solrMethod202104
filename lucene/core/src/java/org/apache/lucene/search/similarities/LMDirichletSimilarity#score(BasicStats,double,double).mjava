  @Override
  protected double score(BasicStats stats, double freq, double docLen) {
    double score =
        stats.getBoost()
            * (Math.log(1 + freq / (mu * ((LMStats) stats).getCollectionProbability()))
                + Math.log(mu / (docLen + mu)));
    return score > 0.0d ? score : 0.0d;
  }

