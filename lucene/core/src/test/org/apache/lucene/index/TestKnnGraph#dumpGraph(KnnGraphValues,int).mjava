  private void dumpGraph(KnnGraphValues values, int size) throws IOException {
    for (int node = 0; node < size; node++) {
      int n;
      System.out.print("" + node + ":");
      values.seek(node);
      while ((n = values.nextNeighbor()) != NO_MORE_DOCS) {
        System.out.print(" " + n);
      }
      System.out.println();
    }
  }

