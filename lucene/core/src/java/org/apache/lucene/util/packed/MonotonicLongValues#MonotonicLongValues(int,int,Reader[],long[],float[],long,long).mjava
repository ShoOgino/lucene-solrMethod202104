  MonotonicLongValues(
      int pageShift,
      int pageMask,
      Reader[] values,
      long[] mins,
      float[] averages,
      long size,
      long ramBytesUsed) {
    super(pageShift, pageMask, values, mins, size, ramBytesUsed);
    assert values.length == averages.length;
    this.averages = averages;
  }

