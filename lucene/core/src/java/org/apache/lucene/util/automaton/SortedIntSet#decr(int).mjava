  // Removes this state from the set, if count decrs to 0
  public void decr(int num) {

    if (useTreeMap) {
      final int count = map.get(num);
      if (count == 1) {
        map.remove(num);
      } else {
        map.put(num, count - 1);
      }
      // Fall back to simple arrays once we touch zero again
      if (map.size() == 0) {
        useTreeMap = false;
        upto = 0;
      }
      return;
    }

    for (int i = 0; i < upto; i++) {
      if (values[i] == num) {
        counts[i]--;
        if (counts[i] == 0) {
          final int limit = upto - 1;
          while (i < limit) {
            values[i] = values[i + 1];
            counts[i] = counts[i + 1];
            i++;
          }
          upto = limit;
        }
        return;
      }
    }
    assert false;
  }

