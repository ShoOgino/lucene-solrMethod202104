    boolean hasFlag(int entryId, char flag) {
      if (entryId < 0 || flag == Dictionary.FLAG_UNSET) return false;

      int length = data[entryId];
      for (int i = entryId + 1; i < entryId + 1 + length; i++) {
        char c = data[i];
        if (c == flag) return true;
        if (c > flag) return false;
      }
      return false;
    }

