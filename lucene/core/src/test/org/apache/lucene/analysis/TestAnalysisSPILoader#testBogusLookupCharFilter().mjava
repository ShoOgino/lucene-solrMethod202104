  public void testBogusLookupCharFilter() {
    expectThrows(
        IllegalArgumentException.class,
        () -> {
          CharFilterFactory.forName("sdfsdfsdfdsfsdfsdf", new HashMap<String, String>());
        });

    expectThrows(
        IllegalArgumentException.class,
        () -> {
          CharFilterFactory.forName("!(**#$U*#$*", new HashMap<String, String>());
        });
  }

