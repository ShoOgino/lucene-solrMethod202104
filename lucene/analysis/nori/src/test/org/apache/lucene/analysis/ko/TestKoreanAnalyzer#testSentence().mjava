  public void testSentence() throws IOException {
    Analyzer a = new KoreanAnalyzer();
    assertAnalyzesTo(
        a,
        "한국은 대단한 나라입니다.",
        new String[] {"한국", "대단", "나라", "이"},
        new int[] {0, 4, 8, 10},
        new int[] {2, 6, 10, 13},
        new int[] {1, 2, 3, 1});
    a.close();
  }

