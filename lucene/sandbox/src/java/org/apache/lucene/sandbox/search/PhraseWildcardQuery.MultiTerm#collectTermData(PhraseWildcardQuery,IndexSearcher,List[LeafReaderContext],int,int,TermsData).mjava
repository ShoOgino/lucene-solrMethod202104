    @Override
    protected int collectTermData(
        PhraseWildcardQuery query,
        IndexSearcher searcher,
        List<LeafReaderContext> segments,
        int remainingMultiTerms,
        int maxExpansionsForTerm,
        TermsData termsData)
        throws IOException {
