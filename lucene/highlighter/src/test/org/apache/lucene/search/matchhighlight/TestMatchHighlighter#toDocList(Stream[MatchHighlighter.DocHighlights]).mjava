  private List<List<String>> toDocList(Stream<MatchHighlighter.DocHighlights> highlights) {
    return highlights
        .map(
            docHighlights ->
                docHighlights.fields.entrySet().stream()
                    .map(e -> e.getKey() + ": " + String.join(", ", e.getValue()))
                    .collect(Collectors.toList()))
        .collect(Collectors.toList());
  }

