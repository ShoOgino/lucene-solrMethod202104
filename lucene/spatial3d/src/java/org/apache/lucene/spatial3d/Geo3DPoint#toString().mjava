  @Override
  public String toString() {
    StringBuilder result = new StringBuilder();
    result.append(getClass().getSimpleName());
    result.append(" <");
    result.append(name);
    result.append(':');

    BytesRef bytes = (BytesRef) fieldsData;
    result.append(" x=").append(decodeDimension(bytes.bytes, bytes.offset, this.planetModel));
    result
        .append(" y=")
        .append(decodeDimension(bytes.bytes, bytes.offset + Integer.BYTES, this.planetModel));
    result
        .append(" z=")
        .append(decodeDimension(bytes.bytes, bytes.offset + 2 * Integer.BYTES, this.planetModel));
    result.append('>');
    return result.toString();
  }

