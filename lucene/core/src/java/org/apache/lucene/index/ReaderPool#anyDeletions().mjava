  /**
   * Returns <code>true</code> iff any of the buffered readers and updates has at least one pending
   * delete
   */
  synchronized boolean anyDeletions() {
    for (ReadersAndUpdates rld : readerMap.values()) {
      if (rld.getDelCount() > 0) {
        return true;
      }
    }
    return false;
  }

