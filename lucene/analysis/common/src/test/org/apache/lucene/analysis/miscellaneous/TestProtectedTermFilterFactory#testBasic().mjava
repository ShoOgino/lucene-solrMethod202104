  public void testBasic() throws Exception {
    String str = "Foo Clara Bar David";
    TokenStream stream = whitespaceMockTokenizer(str);
    stream =
        tokenFilterFactory(
                "ProtectedTerm",
                "ignoreCase",
                "true",
                "protected",
                "protected-1.txt",
                "wrappedFilters",
                "lowercase")
            .create(stream);
    assertTokenStreamContents(stream, new String[] {"Foo", "clara", "Bar", "david"});
  }

