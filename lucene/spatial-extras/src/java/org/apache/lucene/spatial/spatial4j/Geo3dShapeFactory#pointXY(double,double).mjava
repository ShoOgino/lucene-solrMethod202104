  @Override
  public Point pointXY(double x, double y) {
    GeoPointShape point =
        GeoPointShapeFactory.makeGeoPointShape(
            planetModel,
            y * DistanceUtils.DEGREES_TO_RADIANS,
            x * DistanceUtils.DEGREES_TO_RADIANS);
    return new Geo3dPointShape(point, context);
  }

