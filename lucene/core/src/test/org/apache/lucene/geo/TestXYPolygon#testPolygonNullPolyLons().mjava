  /** null y not allowed */
  public void testPolygonNullPolyLons() {
    IllegalArgumentException expected =
        expectThrows(
            IllegalArgumentException.class,
            () -> {
              new XYPolygon(new float[] {18, 18, 19, 19, 18}, null);
            });
    assertTrue(expected.getMessage().contains("y must not be null"));
  }

