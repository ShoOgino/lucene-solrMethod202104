  public void testBogusLookupTokenizerClass() {
    expectThrows(
        IllegalArgumentException.class,
        () -> {
          TokenizerFactory.lookupClass("sdfsdfsdfdsfsdfsdf");
        });

    expectThrows(
        IllegalArgumentException.class,
        () -> {
          TokenizerFactory.lookupClass("!(**#$U*#$*");
        });
  }

