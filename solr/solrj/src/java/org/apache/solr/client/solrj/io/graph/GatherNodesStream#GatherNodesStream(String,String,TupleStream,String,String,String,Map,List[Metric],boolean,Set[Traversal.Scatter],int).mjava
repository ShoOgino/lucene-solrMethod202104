  public GatherNodesStream(String zkHost,
                           String collection,
                           TupleStream tupleStream,
                           String traverseFrom,
                           String traverseTo,
                           String gather,
                           @SuppressWarnings({"rawtypes"})Map queryParams,
                           List<Metric> metrics,
                           boolean trackTraversal,
                           Set<Traversal.Scatter> scatter,
                           int maxDocFreq) {

    init(zkHost,
        collection,
        tupleStream,
        traverseFrom,
        traverseTo,
        gather,
        queryParams,
        metrics,
        trackTraversal,
        scatter,
        maxDocFreq,
        Integer.MIN_VALUE,
    1);
