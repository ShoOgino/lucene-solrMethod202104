  public void testOneInputMultipleOutputNoKeepOrig() throws Exception {
    SynonymMap.Builder b = new SynonymMap.Builder();
    add(b, "a b", "x", false);
    add(b, "a b", "y", false);

    Analyzer a = getAnalyzer(b, true);
    assertAnalyzesTo(
        a,
        "c a b c",
        new String[] {"c", "x", "y", "c"},
        new int[] {0, 2, 2, 6},
        new int[] {1, 5, 5, 7},
        new String[] {"word", "SYNONYM", "SYNONYM", "word"},
        new int[] {1, 1, 0, 1},
        new int[] {1, 1, 1, 1});
    a.close();
  }

