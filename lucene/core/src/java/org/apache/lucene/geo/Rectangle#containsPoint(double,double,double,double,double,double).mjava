  /**
   * returns true if rectangle (defined by minLat, maxLat, minLon, maxLon) contains the lat lon
   * point
   */
  public static boolean containsPoint(
      final double lat,
      final double lon,
      final double minLat,
      final double maxLat,
      final double minLon,
      final double maxLon) {
    return lat >= minLat && lat <= maxLat && lon >= minLon && lon <= maxLon;
  }

