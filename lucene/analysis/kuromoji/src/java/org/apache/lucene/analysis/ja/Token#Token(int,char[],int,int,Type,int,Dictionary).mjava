  public Token(
      int wordId,
      char[] surfaceForm,
      int offset,
      int length,
      Type type,
      int position,
      Dictionary dictionary) {
    this.wordId = wordId;
    this.surfaceForm = surfaceForm;
    this.offset = offset;
    this.length = length;
    this.type = type;
    this.position = position;
    this.dictionary = dictionary;
  }

