    MultiBufferImpl(
        String resourceDescription,
        ByteBuffer[] buffers,
        int offset,
        long length,
        int chunkSizePower,
        ByteBufferGuard guard) {
      super(resourceDescription, buffers, length, chunkSizePower, guard);
      this.offset = offset;
      try {
        seek(0L);
      } catch (IOException ioe) {
        throw new AssertionError(ioe);
      }
    }

