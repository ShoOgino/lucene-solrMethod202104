  @Test
  public void testParamSubclusters() throws Exception {
    compareToExpected("off", clusters("mock", QUERY_TESTSET_SAMPLE_DOCUMENTS, params -> {
      params.set(EngineParameters.PARAM_INCLUDE_SUBCLUSTERS, false);
