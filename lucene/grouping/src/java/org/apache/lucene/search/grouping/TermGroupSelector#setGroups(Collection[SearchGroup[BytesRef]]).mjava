  @Override
  public void setGroups(Collection<SearchGroup<BytesRef>> searchGroups) {
    this.values.clear();
    this.values.reinit();
    for (SearchGroup<BytesRef> sg : searchGroups) {
      if (sg.groupValue == null) {
        includeEmpty = true;
      } else {
        this.values.add(sg.groupValue);
      }
    }
    this.secondPass = true;
  }

