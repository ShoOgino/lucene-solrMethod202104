  private boolean arrayContains(int[] expected, int[] actual) {
    // in some cases the actual docs aren't sorted by docid. This method expects that.
    Arrays.sort(actual);
    if (expected.length != actual.length) {
      return false;
    }

    for (int e : expected) {
      boolean found = false;
      for (int a : actual) {
        if (e == a) {
          found = true;
          break;
        }
      }

      if (!found) {
        return false;
      }
    }

    return true;
  }

