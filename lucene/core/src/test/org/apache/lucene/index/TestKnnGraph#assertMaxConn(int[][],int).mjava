  private void assertMaxConn(int[][] graph, int maxConn) {
    for (int i = 0; i < graph.length; i++) {
      if (graph[i] != null) {
        assert (graph[i].length <= maxConn);
        for (int j = 0; j < graph[i].length; j++) {
          int k = graph[i][j];
          assertNotNull(graph[k]);
        }
      }
    }
  }

