  public void testDoNotIgnoreCase() throws Exception {
    SynonymMap.Builder b = new SynonymMap.Builder();
    add(b, "a b", "x y", false);
    add(b, "a b", "m n o", false);

    Analyzer a = getAnalyzer(b, false);
    assertAnalyzesTo(
        a,
        "c A B D",
        new String[] {"c", "A", "B", "D"},
        new int[] {0, 2, 4, 6},
        new int[] {1, 3, 5, 7},
        new String[] {"word", "word", "word", "word"},
        new int[] {1, 1, 1, 1},
        new int[] {1, 1, 1, 1});
    a.close();
  }

