  private void browseByTerm() {
    String field = getSelectedField();
    String term = getSelectedTerm();
    operatorRegistry
        .get(DocumentsTabOperator.class)
        .ifPresent(
            operator -> {
              operator.browseTerm(field, term);
              tabSwitcher.switchTab(TabbedPaneProvider.Tab.DOCUMENTS);
            });
  }

