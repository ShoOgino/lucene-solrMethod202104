  /** Used by clone() and DrillSideways */
  DrillDownQuery(
      FacetsConfig config,
      Query baseQuery,
      List<BooleanQuery.Builder> dimQueries,
      Map<String, Integer> drillDownDims) {
    this.baseQuery = baseQuery;
    this.dimQueries.addAll(dimQueries);
    this.drillDownDims.putAll(drillDownDims);
    this.config = config;
  }

