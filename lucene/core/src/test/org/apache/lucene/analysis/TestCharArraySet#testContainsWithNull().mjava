  /** Test for NPE */
  public void testContainsWithNull() {
    CharArraySet set = new CharArraySet(1, true);

    expectThrows(
        NullPointerException.class,
        () -> {
          set.contains((char[]) null, 0, 10);
        });

    expectThrows(
        NullPointerException.class,
        () -> {
          set.contains((CharSequence) null);
        });

    expectThrows(
        NullPointerException.class,
        () -> {
          set.contains((Object) null);
        });
  }

