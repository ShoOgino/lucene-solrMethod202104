    HnswGraphFieldEntry(DataInput input, VectorValues.SearchStrategy searchStrategy)
        throws IOException {
      super(input, searchStrategy);
      ordOffsets = new long[size()];
      long offset = 0;
      for (int i = 0; i < ordOffsets.length; i++) {
        offset += input.readVLong();
        ordOffsets[i] = offset;
      }
    }

