  public void testNotContainedByQuery() throws IOException {
    Query q =
        new IntervalQuery(
            field,
            Intervals.notContainedBy(
                Intervals.term("w2"),
                Intervals.unordered(Intervals.term("w1"), Intervals.term("w4"))));
    checkHits(q, new int[] {1, 3, 4, 5});
  }

