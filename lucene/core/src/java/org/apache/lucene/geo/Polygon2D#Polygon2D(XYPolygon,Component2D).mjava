  private Polygon2D(XYPolygon polygon, Component2D holes) {
    this(
        polygon.minX,
        polygon.maxX,
        polygon.minY,
        polygon.maxY,
        XYEncodingUtils.floatArrayToDoubleArray(polygon.getPolyX()),
        XYEncodingUtils.floatArrayToDoubleArray(polygon.getPolyY()),
        holes);
  }

