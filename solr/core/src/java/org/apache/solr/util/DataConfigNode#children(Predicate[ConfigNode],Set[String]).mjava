  @Override
  public List<ConfigNode> children(Predicate<ConfigNode> test, Set<String> matchNames) {
    List<ConfigNode> result = new ArrayList<>();
    for (String s : matchNames) {
      List<ConfigNode> vals = kids.get(s);
      if (vals != null) {
        vals.forEach(it -> {
          if (test == null || test.test(it)) {
            result.add(it);
          }
        });
      }
    }
    return result;
  }

