  public FSTTester(
      Random random,
      Directory dir,
      int inputMode,
      List<InputOutput<T>> pairs,
      Outputs<T> outputs,
      boolean doReverseLookup) {
    this.random = random;
    this.dir = dir;
    this.inputMode = inputMode;
    this.pairs = pairs;
    this.outputs = outputs;
    this.doReverseLookup = doReverseLookup;
  }

