  @Override
  public String toString() {
    String goalDesc = isMinimumLength ? "minLen" : "targetLen";
    return getClass().getSimpleName()
        + "{"
        + goalDesc
        + "="
        + lengthGoal
        + ", fragAlign="
        + fragmentAlignment
        + ", baseIter="
        + baseIter
        + "}";
  }

