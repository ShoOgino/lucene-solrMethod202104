  @Override
  public String toString(String field) {
    return "frange("
        + valueSource
        + "):"
        + (includeLower ? '[' : '{')
        + (lowerVal == null ? "*" : lowerVal)
        + " TO "
        + (upperVal == null ? "*" : upperVal)
        + (includeUpper ? ']' : '}');
  }

