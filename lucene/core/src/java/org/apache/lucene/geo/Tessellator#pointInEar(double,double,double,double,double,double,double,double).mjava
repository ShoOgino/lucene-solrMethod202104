  /** Compute whether point is in a candidate ear */
  private static boolean pointInEar(
      final double x,
      final double y,
      final double ax,
      final double ay,
      final double bx,
      final double by,
      final double cx,
      final double cy) {
    return (cx - x) * (ay - y) - (ax - x) * (cy - y) >= 0
        && (ax - x) * (by - y) - (bx - x) * (ay - y) >= 0
        && (bx - x) * (cy - y) - (cx - x) * (by - y) >= 0;
  }

