  @Override
  public String toString() {
    StringBuilder sb = new StringBuilder();
    sb.append("LINE(");
    for (int i = 0; i < lats.length; i++) {
      sb.append("[")
          .append(lons[i])
          .append(", ")
          .append(lats[i])
          .append("]");
    }
    sb.append(')');
    return sb.toString();
  }

