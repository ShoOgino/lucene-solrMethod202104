  @Override
  public boolean equals(Object o) {
    if (this == o) {
      return true;
    }
    if (o == null || getClass() != o.getClass()) {
      return false;
    }
    MetricImpl<?> that = (MetricImpl<?>) o;
    return name.equals(that.getName()) && internalName.equals(that.getInternalName()) && converter.equals(that.converter);
  }

