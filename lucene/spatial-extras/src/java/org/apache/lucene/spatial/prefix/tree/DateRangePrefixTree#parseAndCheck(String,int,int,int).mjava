  private int parseAndCheck(String str, int offset, int min, int max) {
    int val = Integer.parseInt(str.substring(offset, offset + 2));
    if (val < min || val > max) {
      throw new IllegalArgumentException(
          "Invalid value: " + val + "," + " expecting from " + min + " to " + max + "]");
    }
    return val;
  }

