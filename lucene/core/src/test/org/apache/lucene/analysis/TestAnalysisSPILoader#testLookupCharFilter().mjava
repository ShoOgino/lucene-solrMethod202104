  public void testLookupCharFilter() {
    assertSame(
        FakeCharFilterFactory.class,
        CharFilterFactory.forName("Fake", versionArgOnly()).getClass());
    assertSame(
        FakeCharFilterFactory.class,
        CharFilterFactory.forName("FAKE", versionArgOnly()).getClass());
    assertSame(
        FakeCharFilterFactory.class,
        CharFilterFactory.forName("fake", versionArgOnly()).getClass());
  }

