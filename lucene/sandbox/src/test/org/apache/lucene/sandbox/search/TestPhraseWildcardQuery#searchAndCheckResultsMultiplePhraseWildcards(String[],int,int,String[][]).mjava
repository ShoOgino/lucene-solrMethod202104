  protected void searchAndCheckResultsMultiplePhraseWildcards(
      String[] fields, int maxExpansions, int slop, String[][] multiPhraseTerms)
      throws IOException {
    searchAndCheckResultsMultiplePhraseWildcards(
        fields, maxExpansions, slop, false, multiPhraseTerms);
    searchAndCheckResultsMultiplePhraseWildcards(
        fields, maxExpansions, slop, true, multiPhraseTerms);
  }

