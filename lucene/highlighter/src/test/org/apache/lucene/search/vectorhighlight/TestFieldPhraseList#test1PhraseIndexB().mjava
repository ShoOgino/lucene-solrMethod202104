  public void test1PhraseIndexB() throws Exception {
    // 01 12 23 34 45 56 67 78 (offsets)
    // bb|bb|ba|ac|cb|ba|ab|bc
    //  0  1  2  3  4  5  6  7 (positions)
    make1d1fIndexB("bbbacbabc");

    FieldQuery fq = new FieldQuery(pqF("ba", "ac"), true, true);
