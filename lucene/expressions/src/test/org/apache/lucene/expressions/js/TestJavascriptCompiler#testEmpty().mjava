  public void testEmpty() {
    expectThrows(
        ParseException.class,
        () -> {
          JavascriptCompiler.compile("");
        });

    expectThrows(
        ParseException.class,
        () -> {
          JavascriptCompiler.compile("()");
        });

    expectThrows(
        ParseException.class,
        () -> {
          JavascriptCompiler.compile("   \r\n   \n \t");
        });
  }

