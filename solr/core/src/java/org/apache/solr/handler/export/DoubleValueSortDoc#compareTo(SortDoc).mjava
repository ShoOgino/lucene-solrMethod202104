  @Override
  public int compareTo(SortDoc o) {
    DoubleValueSortDoc sd = (DoubleValueSortDoc) o;
    int comp = value1.compareTo(sd.value1);
    if (comp == 0) {
      comp = value2.compareTo(sd.value2);
      if (comp == 0) {
        return (sd.docId + sd.docBase) - (docId + docBase);
      } else {
        return comp;
      }
    } else {
      return comp;
    }
  }

