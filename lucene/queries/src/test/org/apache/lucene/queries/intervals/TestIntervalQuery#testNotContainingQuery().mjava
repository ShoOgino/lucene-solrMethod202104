  public void testNotContainingQuery() throws IOException {
    Query q =
        new IntervalQuery(
            field,
            Intervals.notContaining(
                Intervals.unordered(Intervals.term("w1"), Intervals.term("w2")),
                Intervals.term("w3")));
    checkHits(q, new int[] {0, 2, 4, 5});
  }

