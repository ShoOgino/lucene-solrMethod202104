  private final void upHeap(int origPos) {
    int i = origPos;
    long value = heap[i];         // save bottom value
    int j = i >>> 1;
    while (j > 0 && lessThan(value, heap[j])) {
      heap[i] = heap[j];       // shift parents down
      i = j;
      j = j >>> 1;
    }
    heap[i] = value;            // install saved value
  }

