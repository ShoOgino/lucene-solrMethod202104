  public void testNestedOrInUnorderedMaxGaps() throws IOException {
    Query q =
        new IntervalQuery(
            field,
            Intervals.maxgaps(
                1,
                Intervals.unordered(
                    Intervals.or(
                        Intervals.term("coordinate"), Intervals.phrase("coordinate", "genome")),
                    Intervals.term("research"))));
    checkHits(q, new int[] {6, 7});
  }

