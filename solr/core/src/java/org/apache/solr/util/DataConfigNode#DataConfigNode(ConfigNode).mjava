  public DataConfigNode(ConfigNode root) {
    name = root.name();
    attributes = wrap(root.attributes());
    textData = root.textValue();
    root.forEachChild(it -> {
      List<ConfigNode> nodes = kids.computeIfAbsent(it.name(),
          k -> new ArrayList<>());

     nodes.add(new DataConfigNode(it));
      return Boolean.TRUE;
    });

  }

