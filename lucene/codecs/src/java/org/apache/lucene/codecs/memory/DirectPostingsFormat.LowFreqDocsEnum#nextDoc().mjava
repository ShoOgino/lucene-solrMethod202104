    // TODO: can do this w/o setting members?
    @Override
    public int nextDoc() {
      upto += 2 + freq * posMult;
      // if (DEBUG) {
      //   System.out.println("  nextDoc freq=" + freq + " upto=" + upto + " vs " +
      // postings.length);
      // }
      if (upto < postings.length) {
        freq = postings[upto + 1];
        assert freq > 0;
        return postings[upto];
      }
      return NO_MORE_DOCS;
    }

