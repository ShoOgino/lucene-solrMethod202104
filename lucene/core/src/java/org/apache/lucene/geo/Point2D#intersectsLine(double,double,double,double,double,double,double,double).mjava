  @Override
  public boolean intersectsLine(
      double minX,
      double maxX,
      double minY,
      double maxY,
      double aX,
      double aY,
      double bX,
      double bY) {
    return Component2D.containsPoint(x, y, minX, maxX, minY, maxY)
        && orient(aX, aY, bX, bY, x, y) == 0;
  }

