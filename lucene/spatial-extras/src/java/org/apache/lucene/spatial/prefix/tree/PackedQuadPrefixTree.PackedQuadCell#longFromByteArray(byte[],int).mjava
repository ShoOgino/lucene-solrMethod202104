    private long longFromByteArray(byte[] bytes, int ofs) {
      assert bytes.length >= 8;
      return fromBytes(
          bytes[0 + ofs],
          bytes[1 + ofs],
          bytes[2 + ofs],
          bytes[3 + ofs],
          bytes[4 + ofs],
          bytes[5 + ofs],
          bytes[6 + ofs],
          bytes[7 + ofs]);
    }

