  @Override
  public Point getCenter() {
    Point center = this.center; // volatile read once
    if (center == null) {
      GeoPoint point = shape.getCenter();
      center =
          new Geo3dPointShape(
              GeoPointShapeFactory.makeGeoPointShape(
                  shape.getPlanetModel(), point.getLatitude(), point.getLongitude()),
              spatialcontext);
      this.center = center;
    }
    return center;
  }

