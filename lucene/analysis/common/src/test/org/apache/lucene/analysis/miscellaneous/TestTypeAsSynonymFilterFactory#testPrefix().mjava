  public void testPrefix() throws Exception {
    TokenStream stream = new CannedTokenStream(TOKENS);
    stream = tokenFilterFactory("TypeAsSynonym", "prefix", "_type_").create(stream);
    assertTokenStreamContents(
        stream,
        new String[] {"Visit", "_type_<ALPHANUM>", "example.com", "_type_<URL>"},
        null,
        null,
        new String[] {"<ALPHANUM>", "<ALPHANUM>", "<URL>", "<URL>"},
        new int[] {1, 0, 1, 0});
  }

