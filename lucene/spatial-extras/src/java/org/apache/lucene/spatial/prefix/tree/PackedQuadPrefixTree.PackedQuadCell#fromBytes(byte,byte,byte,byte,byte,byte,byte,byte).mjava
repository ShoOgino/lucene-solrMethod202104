    private long fromBytes(byte b1, byte b2, byte b3, byte b4, byte b5, byte b6, byte b7, byte b8) {
      return ((long) b1 & 255L) << 56
          | ((long) b2 & 255L) << 48
          | ((long) b3 & 255L) << 40
          | ((long) b4 & 255L) << 32
          | ((long) b5 & 255L) << 24
          | ((long) b6 & 255L) << 16
          | ((long) b7 & 255L) << 8
          | (long) b8 & 255L;
    }

