    @Override
    public String toString(int depth) {
      StringBuilder sb =
          new StringBuilder(space(depth))
              .append("Conjunction[")
              .append(children.size())
              .append("]^")
              .append(weight())
              .append("\n");
      for (QueryTree child : children) {
        sb.append(child.toString(depth + 2)).append("\n");
      }
      return sb.toString();
    }

