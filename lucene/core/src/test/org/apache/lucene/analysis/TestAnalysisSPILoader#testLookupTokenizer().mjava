  public void testLookupTokenizer() {
    assertSame(
        StandardTokenizerFactory.class,
        TokenizerFactory.forName("Standard", versionArgOnly()).getClass());
    assertSame(
        StandardTokenizerFactory.class,
        TokenizerFactory.forName("STANDARD", versionArgOnly()).getClass());
