  /** (internal) Returns the slot for this key */
  public int getSlot(int key) {
    assert key != emptyVal;
    int h = hash(key);
    int s = h & (keys.length - 1);
    if (keys[s] == key || keys[s] == emptyVal) return s;

    int increment = (h >> 7) | 1;
    do {
      s = (s + increment) & (keys.length - 1);
    } while (keys[s] != key && keys[s] != emptyVal);
    return s;
  }

