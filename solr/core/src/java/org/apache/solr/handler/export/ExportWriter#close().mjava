  @Override
  public void close() throws IOException {
    if (writer != null) {
      try {
        writer.close();
      } catch (Throwable t) {
        //We're going to sit on this.
      }
    }
    if (respWriter != null) {
      try {
        respWriter.flush();
        respWriter.close();
      } catch (Throwable t) {

      }
    }

  }

