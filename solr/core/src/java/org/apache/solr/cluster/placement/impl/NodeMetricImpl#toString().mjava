  @Override
  public String toString() {
    if (registry != null) {
      return "NodeMetricImpl{" +
          "name='" + name + '\'' +
          ", internalName='" + internalName + '\'' +
          ", converter=" + converter +
          ", registry=" + registry +
          '}';
    } else {
      return "NodeMetricImpl{key=" + internalName + "}";
    }
  }

