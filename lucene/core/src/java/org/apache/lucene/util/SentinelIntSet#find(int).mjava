  /** (internal) Returns the slot for this key, or -slot-1 if not found */
  public int find(int key) {
    assert key != emptyVal;
    int h = hash(key);
    int s = h & (keys.length - 1);
    if (keys[s] == key) return s;
    if (keys[s] == emptyVal) return -s - 1;

    int increment = (h >> 7) | 1;
    for (; ; ) {
      s = (s + increment) & (keys.length - 1);
      if (keys[s] == key) return s;
      if (keys[s] == emptyVal) return -s - 1;
    }
  }

