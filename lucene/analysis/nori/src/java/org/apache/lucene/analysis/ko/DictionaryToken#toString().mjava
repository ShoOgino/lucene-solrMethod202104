  @Override
  public String toString() {
    return "DictionaryToken(\""
        + getSurfaceFormString()
        + "\" pos="
        + getStartOffset()
        + " length="
        + getLength()
        + " posLen="
        + getPositionLength()
        + " type="
        + type
        + " wordId="
        + wordId
        + " leftID="
        + dictionary.getLeftId(wordId)
        + ")";
  }

