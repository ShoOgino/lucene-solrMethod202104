  private String intValueToStringValue(Integer intVal) {
    if (intVal == null) {
      return null;
    }

    final String enumString = enumIntToStringMap.get(intVal);
    if (enumString != null) {
      return enumString;
    }
    // can't find matching enum name - return DEFAULT_VALUE.toString()
    return DEFAULT_VALUE.toString();
  }

