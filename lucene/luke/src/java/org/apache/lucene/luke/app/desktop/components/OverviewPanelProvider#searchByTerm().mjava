  private void searchByTerm() {
    String field = getSelectedField();
    String term = getSelectedTerm();
    operatorRegistry
        .get(SearchTabOperator.class)
        .ifPresent(
            operator -> {
              operator.searchByTerm(field, term);
              tabSwitcher.switchTab(TabbedPaneProvider.Tab.SEARCH);
            });
  }

