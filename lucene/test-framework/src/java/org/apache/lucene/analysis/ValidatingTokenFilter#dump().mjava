  public String dump() {
    StringBuilder buf = new StringBuilder();
    buf.append(name).append(": ");
    for (Token token : tokens) {
      buf.append(
          String.format(
              Locale.ROOT,
              "%s<[%d-%d] +%d> ",
              token,
              token.startOffset(),
              token.endOffset(),
              token.getPositionIncrement()));
    }
    return buf.toString();
  }

