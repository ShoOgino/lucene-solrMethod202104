  public void testUnmodifiable() {
    final Iterator<String> it =
        new FilterIterator<String, String>(set.iterator()) {
          @Override
          protected boolean predicateFunction(String s) {
            return true;
          }
        };
    assertEquals("a", it.next());
    expectThrows(
        UnsupportedOperationException.class,
        () -> {
          it.remove();
        });
  }

