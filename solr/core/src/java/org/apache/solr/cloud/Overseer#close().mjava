  public synchronized void close() {
    if (this.id != null) {
      log.info("Overseer (id={}) closing", id);
    }
    // stop singletons only on the leader
    if (!this.closed) {
      getCoreContainer().getClusterSingletons().stopClusterSingletons();
    }
    this.closed = true;
    doClose();


    assert ObjectReleaseTracker.release(this);
  }

