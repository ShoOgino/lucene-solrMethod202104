    public PendingBlock(
        BytesRef prefix,
        long fp,
        boolean hasTerms,
        long totalTermCount,
        boolean isFloor,
        int floorLeadByte,
        List<SubIndex> subIndices) {
      super(false);
      this.prefix = prefix;
      this.fp = fp;
      this.hasTerms = hasTerms;
      this.totalTermCount = totalTermCount;
      assert totalTermCount > 0;
      this.isFloor = isFloor;
      this.floorLeadByte = floorLeadByte;
      this.subIndices = subIndices;
    }

