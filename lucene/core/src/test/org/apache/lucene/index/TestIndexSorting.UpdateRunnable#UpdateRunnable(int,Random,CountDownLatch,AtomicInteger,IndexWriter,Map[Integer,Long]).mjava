    UpdateRunnable(
        int numDocs,
        Random random,
        CountDownLatch latch,
        AtomicInteger updateCount,
        IndexWriter w,
        Map<Integer, Long> values) {
      this.numDocs = numDocs;
      this.random = random;
      this.latch = latch;
      this.updateCount = updateCount;
      this.w = w;
      this.values = values;
    }

