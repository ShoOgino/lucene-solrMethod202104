    @Override
    public void onCommit(List<? extends IndexCommit> commits) throws IOException {
      verifyCommitOrder(commits);
      int size = commits.size();
      // Delete all but last one:
      for (int i = 0; i < size - 1; i++) {
        ((IndexCommit) commits.get(i)).delete();
      }
      numOnCommit++;
    }

