  public void testLookaheadAfterParse() throws Exception {
    SynonymMap.Builder b = new SynonymMap.Builder();
    add(b, "b b", "x", true);
    add(b, "b", "y", true);

    Analyzer a = getAnalyzer(b, true);

    assertAnalyzesTo(
        a,
        "b a b b",
        new String[] {"y", "b", "a", "x", "b", "b"},
        new int[] {0, 0, 2, 4, 4, 6},
        new int[] {1, 1, 3, 7, 5, 7},
        null,
        new int[] {1, 0, 1, 1, 0, 1},
        new int[] {1, 1, 1, 2, 1, 1},
        true);
  }

