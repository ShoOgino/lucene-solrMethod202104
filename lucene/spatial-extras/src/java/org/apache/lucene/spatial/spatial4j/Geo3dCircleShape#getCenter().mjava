  @Override
  public Point getCenter() {
    Point center = this.center; // volatile read once
    if (center == null) {
      center =
          new Geo3dPointShape(
              GeoPointShapeFactory.makeGeoPointShape(
                  shape.getPlanetModel(),
                  shape.getCenter().getLatitude(),
                  shape.getCenter().getLongitude()),
              spatialcontext);
      this.center = center;
    }
    return center;
  }

