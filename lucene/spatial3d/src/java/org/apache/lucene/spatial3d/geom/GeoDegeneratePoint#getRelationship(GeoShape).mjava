  @Override
  public int getRelationship(final GeoShape shape) {
    if (shape.isWithin(this)) {
      // System.err.println("Degenerate point " + this + " is WITHIN shape " + shape);
      return CONTAINS;
    }

    // System.err.println("Degenerate point " + this + " is NOT within shape " + shape);
    return DISJOINT;
  }

