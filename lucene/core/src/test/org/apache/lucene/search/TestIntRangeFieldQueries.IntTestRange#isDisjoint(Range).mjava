    @Override
    protected boolean isDisjoint(Range o) {
      IntTestRange other = (IntTestRange) o;
      for (int d = 0; d < this.min.length; ++d) {
        if (this.min[d] > other.max[d] || this.max[d] < other.min[d]) {
          // disjoint:
          return true;
        }
      }
      return false;
    }

