  public void testBogusLookupTokenFilter() {
    expectThrows(
        IllegalArgumentException.class,
        () -> {
          TokenFilterFactory.forName("sdfsdfsdfdsfsdfsdf", new HashMap<String, String>());
        });

    expectThrows(
        IllegalArgumentException.class,
        () -> {
          TokenFilterFactory.forName("!(**#$U*#$*", new HashMap<String, String>());
        });
  }

