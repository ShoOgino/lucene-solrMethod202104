  static GenericTermsCollector createCollectorSV(
      Function<BinaryDocValues> svFunction, ScoreMode mode) {

    switch (mode) {
      case None:
        return wrap(new TermsCollector.SV(svFunction));
      case Avg:
        return new SV.Avg(svFunction);
      default:
        return new SV(svFunction, mode);
    }
  }

