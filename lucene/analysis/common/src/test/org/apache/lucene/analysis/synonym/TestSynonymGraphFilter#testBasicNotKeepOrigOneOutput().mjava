  public void testBasicNotKeepOrigOneOutput() throws Exception {
    SynonymMap.Builder b = new SynonymMap.Builder();
    add(b, "a b", "x", false);

    Analyzer a = getAnalyzer(b, true);
    assertAnalyzesTo(
        a,
        "c a b",
        new String[] {"c", "x"},
        new int[] {0, 2},
        new int[] {1, 5},
        new String[] {"word", "SYNONYM"},
        new int[] {1, 1},
        new int[] {1, 1});
    a.close();
  }

