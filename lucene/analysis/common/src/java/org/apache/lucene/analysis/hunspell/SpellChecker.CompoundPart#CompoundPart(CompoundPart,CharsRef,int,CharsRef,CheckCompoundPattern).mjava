    CompoundPart(
        CompoundPart prev, CharsRef tail, int length, CharsRef stem, CheckCompoundPattern enabler) {
      this.prev = prev;
      this.tail = tail;
      this.length = length;
      this.stem = stem;
      index = prev == null ? 1 : prev.index + 1;
      enablingPattern = enabler;
    }

