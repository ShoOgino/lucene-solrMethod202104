  private void fail(
      String label,
      String id,
      List<Shape> indexedShapes,
      Shape queryShape,
      SpatialOperation operation) {
    fail(
        ("[" + operation + "] ")
            + (label + " I#" + id + ":" + indexedShapes.get(Integer.parseInt(id)))
            + (" Q:" + queryShape));
  }

