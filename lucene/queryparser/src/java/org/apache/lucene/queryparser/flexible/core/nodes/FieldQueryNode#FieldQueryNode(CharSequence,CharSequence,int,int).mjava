  /**
   * @param field - field name
   * @param text - value
   * @param begin - position in the query string
   * @param end - position in the query string
   */
  public FieldQueryNode(CharSequence field, CharSequence text, int begin, int end) {
    this.field = field;
    this.text = text;
    this.begin = begin;
    this.end = end;
    this.setLeaf(true);
  }

