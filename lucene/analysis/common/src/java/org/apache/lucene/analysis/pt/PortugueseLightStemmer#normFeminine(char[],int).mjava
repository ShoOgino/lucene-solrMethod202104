  private int normFeminine(char s[], int len) {
    if (len > 7
        && (endsWith(s, len, "inha") || endsWith(s, len, "iaca") || endsWith(s, len, "eira"))) {
      s[len - 1] = 'o';
      return len;
    }

    if (len > 6) {
      if (endsWith(s, len, "osa")
          || endsWith(s, len, "ica")
          || endsWith(s, len, "ida")
          || endsWith(s, len, "ada")
          || endsWith(s, len, "iva")
          || endsWith(s, len, "ama")) {
        s[len - 1] = 'o';
        return len;
      }

      if (endsWith(s, len, "ona")) {
        s[len - 3] = 'ã';
        s[len - 2] = 'o';
        return len - 1;
      }

      if (endsWith(s, len, "ora")) return len - 1;

      if (endsWith(s, len, "esa")) {
        s[len - 3] = 'ê';
        return len - 1;
      }

      if (endsWith(s, len, "na")) {
        s[len - 1] = 'o';
        return len;
      }
    }
    return len;
  }

