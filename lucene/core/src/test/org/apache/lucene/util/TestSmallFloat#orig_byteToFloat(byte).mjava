  // original lucene byteToFloat
  static float orig_byteToFloat(byte b) {
    if (b == 0) // zero is a special case
    return 0.0f;
    int mantissa = b & 7;
    int exponent = (b >> 3) & 31;
    int bits = ((exponent + (63 - 15)) << 24) | (mantissa << 21);
    return Float.intBitsToFloat(bits);
  }

