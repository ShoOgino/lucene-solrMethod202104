  @Override
  public void end() throws IOException {
    if (endState == null) {
      super.end();
      endState = captureState();
    } else {
      restoreState(endState);
    }
    endOffset = getAttribute(OffsetAttribute.class).endOffset();
    if (lastTokenFiltered) {
      this.delegate.end();
      endState = captureState();
    }
  }

