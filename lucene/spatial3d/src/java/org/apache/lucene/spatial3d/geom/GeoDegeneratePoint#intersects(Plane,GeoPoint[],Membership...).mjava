  @Override
  public boolean intersects(
      final Plane plane, final GeoPoint[] notablePoints, final Membership... bounds) {
    // If not on the plane, no intersection
    if (!plane.evaluateIsZero(this)) {
      return false;
    }

    for (Membership m : bounds) {
      if (!m.isWithin(this)) {
        return false;
      }
    }
    return true;
  }

