    @Override
    public void writeMap(EntryWriter ew) throws IOException {
      ew.put(NAME, replica);
      ew.put(VERSION, version);
      ew.put(ZkStateReader.STATE_PROP, state.toString());
      if (isLeader) ew.put(Slice.LEADER, isLeader);
      ew.putIfNotNull("duplicate", duplicate);
    }

