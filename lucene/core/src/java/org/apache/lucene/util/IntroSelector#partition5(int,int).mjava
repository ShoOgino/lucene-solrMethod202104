  // selects the median of a group of at most five elements,
  // implemented using insertion sort. Efficient due to
  // bounded nature of data set.
  private int partition5(int left, int right) {
    int i = left + 1;
    while (i <= right) {
      int j = i;
      while (j > left && compare(j - 1, j) > 0) {
        swap(j - 1, j);
        j--;
      }
      i++;
    }
    return (left + right) >>> 1;
  }

