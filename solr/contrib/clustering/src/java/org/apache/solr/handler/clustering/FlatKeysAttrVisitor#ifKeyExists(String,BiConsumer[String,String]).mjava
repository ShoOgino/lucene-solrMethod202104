  private void ifKeyExists(String key, BiConsumer<String, String> pathConsumer) {
    withKey(key, (path) -> {
      if (attrs.containsKey(path)) {
        String value = attrs.get(path);
        if (value.trim().isEmpty()) {
          value = null;
        }
        pathConsumer.accept(path, value);
      }
    });
  }

