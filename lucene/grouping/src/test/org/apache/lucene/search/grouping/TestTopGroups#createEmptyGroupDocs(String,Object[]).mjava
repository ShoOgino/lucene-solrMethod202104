  private static GroupDocs<String> createEmptyGroupDocs(
      String groupValue, Object[] groupSortValues) {
    return new GroupDocs<String>(
        Float.NaN /* score */,
        Float.NaN /* maxScore */,
        new TotalHits(0, TotalHits.Relation.EQUAL_TO),
        new ScoreDoc[0],
        groupValue,
        groupSortValues);
  }

