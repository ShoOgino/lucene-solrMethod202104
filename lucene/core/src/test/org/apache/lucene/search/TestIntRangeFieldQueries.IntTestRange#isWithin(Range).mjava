    @Override
    protected boolean isWithin(Range o) {
      IntTestRange other = (IntTestRange) o;
      for (int d = 0; d < this.min.length; ++d) {
        if ((this.min[d] >= other.min[d] && this.max[d] <= other.max[d]) == false) {
          // not within:
          return false;
        }
      }
      return true;
    }

