  private static double wrapLon(double lon) {
    // System.out.println("wrapLon " + lon);
    if (lon > 180) {
      // System.out.println("  " + (lon - 360));
      return lon - 360;
    } else if (lon < -180) {
      // System.out.println("  " + (lon + 360));
      return lon + 360;
    } else {
      // System.out.println("  " + lon);
      return lon;
    }
  }

