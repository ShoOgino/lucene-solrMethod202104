  public void testLookaheadSecondParse() throws Exception {
    SynonymMap.Builder b = new SynonymMap.Builder();
    add(b, "b b b", "x", true);
    add(b, "b", "y", true);

    Analyzer a = getAnalyzer(b, true);

    assertAnalyzesTo(
        a,
        "b b",
        new String[] {"y", "b", "y", "b"},
        new int[] {0, 0, 2, 2},
        new int[] {1, 1, 3, 3},
        null,
        new int[] {1, 0, 1, 0},
        new int[] {1, 1, 1, 1},
        true);
  }

