  public DictionaryToken(
      KoreanTokenizer.Type type,
      Dictionary dictionary,
      int wordId,
      char[] surfaceForm,
      int offset,
      int length,
      int startOffset,
      int endOffset) {
    super(surfaceForm, offset, length, startOffset, endOffset);
    this.type = type;
    this.dictionary = dictionary;
    this.wordId = wordId;
  }

