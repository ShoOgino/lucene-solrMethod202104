  private static double wrapLat(double lat) {
    // System.out.println("wrapLat " + lat);
    if (lat > 90) {
      // System.out.println("  " + (180 - lat));
      return 180 - lat;
    } else if (lat < -90) {
      // System.out.println("  " + (-180 - lat));
      return -180 - lat;
    } else {
      // System.out.println("  " + lat);
      return lat;
    }
  }

