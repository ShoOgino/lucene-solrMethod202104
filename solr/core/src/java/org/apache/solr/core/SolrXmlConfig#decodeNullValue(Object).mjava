  private static Object decodeNullValue(Object o) {
    if (o instanceof String) { // check if it's a JSON object
      String str = (String) o;
      if (!str.isBlank() && (str.startsWith("{") || str.startsWith("["))) {
        try {
          o = Utils.fromJSONString((String) o);
        } catch (Exception e) {
          // ignore
        }
      }
    }
    return o;
  }

