  public void testInvalidFloatPointUsage() throws Exception {
    FloatPoint field = new FloatPoint("field", 17, 42);

    expectThrows(
        IllegalArgumentException.class,
        () -> {
          field.setFloatValue(14);
        });

    expectThrows(
        IllegalStateException.class,
        () -> {
          field.numericValue();
        });
  }

