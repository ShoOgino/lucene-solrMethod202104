  @Override
  public boolean reject(Thread t) {
    if (isJ9) {
      // LUCENE-6518
      if ("ClassCache Reaper".equals(t.getName())) {
        return true;
      }

      // LUCENE-4736
      StackTraceElement[] stack = t.getStackTrace();
      if (stack.length > 0
          && stack[stack.length - 1].getClassName().equals("java.util.Timer$TimerImpl")) {
        return true;
      }
    }
    return false;
  }

