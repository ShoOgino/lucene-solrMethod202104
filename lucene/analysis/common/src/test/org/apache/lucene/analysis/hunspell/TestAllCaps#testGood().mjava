  public void testGood() {
    assertStemsTo("OpenOffice.org", "OpenOffice.org");
    assertStemsTo("UNICEF's", "UNICEF");
    assertStemsTo("L'Afrique", "Afrique");
    assertStemsTo("L'AFRIQUE", "Afrique");

    // Hunspell returns these title-cased stems, so for consistency we do, too
    assertStemsTo("OPENOFFICE.ORG", "Openoffice.org");
    assertStemsTo("UNICEF'S", "Unicef");
  }

