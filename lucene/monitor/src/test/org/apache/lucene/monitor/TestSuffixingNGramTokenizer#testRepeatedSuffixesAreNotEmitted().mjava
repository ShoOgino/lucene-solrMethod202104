  public void testRepeatedSuffixesAreNotEmitted() throws IOException {
    assertAnalyzesTo(
        analyzer,
        "arm harm term",
        new String[] {
          "arm", "armXX", "arXX", "aXX", "rmXX", "rXX", "mXX", "XX", "harm", "harmXX", "harXX",
          "haXX", "hXX", "term", "termXX", "terXX", "teXX", "tXX", "ermXX", "erXX", "eXX"
        });
  }

