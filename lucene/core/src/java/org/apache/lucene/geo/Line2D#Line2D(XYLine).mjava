  private Line2D(XYLine line) {
    this.minY = line.minY;
    this.maxY = line.maxY;
    this.minX = line.minX;
    this.maxX = line.maxX;
    this.tree =
        EdgeTree.createTree(
            XYEncodingUtils.floatArrayToDoubleArray(line.getX()),
            XYEncodingUtils.floatArrayToDoubleArray(line.getY()));
  }

