    @Override
    public boolean advanceExact(int target) throws IOException {
      docID = target;
      upto = values.offsets[docID];
      if (values.offsets[docID] > 0) {
        numValues = Math.toIntExact(values.values.get(upto - 1));
        limit = upto + numValues;
        return true;
      } else {
        limit = upto;
      }
      return false;
    }

