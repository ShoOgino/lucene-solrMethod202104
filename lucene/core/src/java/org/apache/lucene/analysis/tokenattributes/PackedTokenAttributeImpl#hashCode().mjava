  @Override
  public int hashCode() {
    int code = super.hashCode();
    code = code * 31 + startOffset;
    code = code * 31 + endOffset;
    code = code * 31 + positionIncrement;
    code = code * 31 + positionLength;
    if (type != null) code = code * 31 + type.hashCode();
    code = code * 31 + termFrequency;
    return code;
  }

