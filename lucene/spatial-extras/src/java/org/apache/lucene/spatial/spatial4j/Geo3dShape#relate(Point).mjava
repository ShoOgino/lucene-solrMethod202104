  private int relate(Point p) {
    GeoPoint point =
        new GeoPoint(
            shape.getPlanetModel(),
            p.getY() * DistanceUtils.DEGREES_TO_RADIANS,
            p.getX() * DistanceUtils.DEGREES_TO_RADIANS);

    if (shape.isWithin(point)) {
      return GeoArea.WITHIN;
    }
    return GeoArea.DISJOINT;
  }

