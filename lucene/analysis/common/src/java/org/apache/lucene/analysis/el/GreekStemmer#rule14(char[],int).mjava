  private int rule14(char s[], int len) {
    boolean removed = false;

    if (len > 5 && endsWith(s, len, "ουσεσ")) {
      len -= 5;
      removed = true;
    } else if (len > 4 && (endsWith(s, len, "ουσα") || endsWith(s, len, "ουσε"))) {
      len -= 4;
      removed = true;
    }

    if (removed
        && (exc14.contains(s, 0, len)
            || endsWithVowel(s, len)
            || endsWith(s, len, "ποδαρ")
            || endsWith(s, len, "βλεπ")
            || endsWith(s, len, "πανταχ")
            || endsWith(s, len, "φρυδ")
            || endsWith(s, len, "μαντιλ")
            || endsWith(s, len, "μαλλ")
            || endsWith(s, len, "κυματ")
            || endsWith(s, len, "λαχ")
            || endsWith(s, len, "ληγ")
            || endsWith(s, len, "φαγ")
            || endsWith(s, len, "ομ")
            || endsWith(s, len, "πρωτ"))) {
      len += 3; // add back -ουσ
    }

    return len;
  }

