  /** The ID for logging purposes. */
  public String getPrintableId() {
    if (req == null) {
      return "(uninitialized)"; // in tests?
    }
    extractIdsIfNeeded();
    if (indexedIdStr == null) {
      return "(null)";
    } else if (indexedIdStr.equals(childDocIdStr)) {
      return indexedIdStr;
    } else {
      return childDocIdStr + " (root=" + indexedIdStr + ")";
    }
  }

