  /** factory method to create a new bounding box query */
  @Override
  protected Query newRectQuery(
      String field,
      QueryRelation queryRelation,
      double minX,
      double maxX,
      double minY,
      double maxY) {
    return XYShape.newBoxQuery(
        field, queryRelation, (float) minX, (float) maxX, (float) minY, (float) maxY);
  }

