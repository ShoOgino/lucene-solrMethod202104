  @Override
  public void decode(
      long[] blocks, int blocksOffset, long[] values, int valuesOffset, int iterations) {
    for (int i = 0; i < iterations; ++i) {
      final long block = blocks[blocksOffset++];
      for (int shift = 63; shift >= 0; shift -= 1) {
        values[valuesOffset++] = (block >>> shift) & 1;
      }
    }
  }

